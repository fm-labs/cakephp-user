{
    "name": "fm-labs/cakephp3-user",
    "description": "User plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "User\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "User\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "suggest": {
        "matomo-org/device-detector": "Enabled User Agents parsing to detect devices, clients, operating systems, brands and models."
    }
}
